{\n  "manifest_version": 3,\n  "name": "IDE Link Interceptor",\n  "version": "1.3.0",\n  "description": "Browser extension that intercepts IDE hyperlinks (vscode://, cursor://, windsurf://) and lets you choose which IDE to open them with",\n  "permissions": [\n    "storage",\n    "contextMenus"\n  ],\n  "host_permissions": [\n    "<all_urls>"\n  ],\n  "background": {\n    "service_worker": "background.js"\n  },\n  "action": {\n    "default_popup": "popup.html",\n    "default_title": "選擇 IDE"\n  },\n  "content_scripts": [\n    {\n      "matches": [\n        "<all_urls>"\n      ],\n      "js": [\n        "content.js"\n      ],\n      "run_at": "document_start",\n      "all_frames": true\n    }\n  ],\n  "icons": {\n    "16": "icons/icon16.png",\n    "48": "icons/icon48.png",\n    "128": "icons/icon128.png"\n  }\n}